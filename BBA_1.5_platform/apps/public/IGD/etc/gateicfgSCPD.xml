"<?xml version="1.0"?>\n"
"<scpd xmlns="urn:schemas-upnp-org:service-1-0">\n"
"	<specVersion>\n"
"		<major>1</major>\n"
"		<minor>0</minor>\n"
"	</specVersion>\n"
"	<actionList>\n"
"    <action>\n"
