"          <relatedStateVariable>TotalPacketsSent</relatedStateVariable>\n"
"        </argument>\n"
"      </argumentList>\n"
"    </action>\n"
"    <action>\n"
"    <name>GetTotalPacketsReceived</name>\n"
"      <argumentList>\n"
"        <argument>\n"
"          <name>NewTotalPacketsReceived</name>\n"
"          <direction>out</direction>\n"
