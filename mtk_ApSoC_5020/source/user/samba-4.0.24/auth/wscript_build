#!/usr/bin/env python

bld.SAMBA_LIBRARY('auth_sam_reply',
                  source='auth_sam_reply.c',
                  deps='talloc samba-security samba-util',
                  autoproto='auth_sam_reply.h',
                  private_library=True
                  )

bld.RECURSE('gensec')
bld.RECURSE('ntlmssp')
bld.RECURSE('credentials')
