	@list='$(doclic_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(doclicdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(doclicdir)/$$f"; \
	done
